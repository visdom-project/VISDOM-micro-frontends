  "dependencies": {
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "async-mqtt": "^2.6.1",
    "axios": "^0.21.2",
    "bootstrap": "^5.1.0",
